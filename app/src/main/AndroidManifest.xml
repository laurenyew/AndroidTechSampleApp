<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="laurenyew.techsampleapp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".roomWithAView.activities.NewWordActivity">
        </activity>
        <activity android:name=".roomWithAView.activities.RoomWithAViewActivity">
        </activity>
        <activity android:name="laurenyew.techsampleapp.rxjava.cheesefinder.CheeseActivity"/>
